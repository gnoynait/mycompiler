var a, b, i:integer;
begin
	read(a);
	read(b);
	if a < b then
		for i := a to b do
			write(i)
	else if a == b then
		write("a is equal to b")
	else 
		for i := a downto b do
			write(i)
end.
___________________________________________________


var a, b:integer;
begin
	read(a);
	read(b);
	if a < b then
		write("a is less than b")
	else if a = b then
		write("a is equal to b")
	else
		write("a is greater than b")
end.




___________________________________________________
const 
	a = 45 , b = 27 ;
var 
	i , x , y : integer ;
	head , foot : array [10] of integer ;

	procedure calc( var head , foot : integer ) ;
	var 
		i , rabbit , cock , mark : integer ;
	begin
		write( " the number of head is " , head ) ;
		write( " the number of head is " , foot ) ;
		mark := 0 ;
		for i := 0 to head do 
		begin
			rabbit := i ;
			cock := head - rabbit ;
			if rabbit * 4 + cock * 2 = foot then 
			begin
				write( " the number of rabbit is " , rabbit ) ;
				write( " the number of cock is " , cock ) ;
				mark := 1 ;
				i := head ;
			end
		end;
		if mark = 0 then 
		begin
			write( " the number of rabbit is " , 0 ) ;
			write( " the number of cock is " , 0 ) ;
		end
	end;

begin
	for i := 1 to 4 do 
	begin
		read( x , y ) ;
		head[i] := x ;
		foot[i] := y ;
		calc( head[i] , foot[i] ) ;
	end;
end.
