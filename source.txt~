var i1, i2, i3:integer;
	c1, c2, c3:char;
begin
	read(i1, i2, c1, c2);
	write("hello");
	write("i1 is ", i1);
	write("c1 is ", c1);
	read(i3);
	read(c3);
	write(i3);
	write(c3)
end.
