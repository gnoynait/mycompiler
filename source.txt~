var i, n:integer;
function fib(n:integer):integer;
	begin
		if n =0 then
			fib := 1
		else if n =1 then
			fib := 1
		else
			fib := fib(n-1) + fib(n-2)
	end;
begin
	read(n);
	for i := 0 to n do
		write(fib(i))
end.

