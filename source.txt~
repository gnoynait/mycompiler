var a:integer;
procedure proc (var b:integer);
	begin
		b := b * 2
	end;
begin
	read(a);
	proc(a);
	write(a)
end.
___________________________________________________
var a:integer;
procedure proc();
	begin
		a := a * 2
	end;
begin
	read(a);
	proc();
	write(a)
end.

